package com.CS477.drinkandgo;

import java.util.ArrayList;

public class Venue 
{
	private final String name, zipCode, city, state;
	private final ArrayList<Drink> menu;
	
	public Venue(String name, String zipCode, String city, String state, ArrayList<Drink> menu)
	{
		this.name = name;
		this.zipCode = zipCode;
		this.city = city;
		this.state = state;
		
		this.menu = new ArrayList<Drink>(menu);
	}

	public String getZipCode() {
		return zipCode;
	}
}
