		// TODO Auto-generated constructor stub
	}

	public OrderDrinksRequest(int method, String url,
			Listener<String> listener, ErrorListener errorListener) {
		super(method, url, listener, errorListener);
		// TODO Auto-generated constructor stub
	}

}
