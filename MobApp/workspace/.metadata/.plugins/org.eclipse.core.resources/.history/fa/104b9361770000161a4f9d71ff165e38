package com.CS477.drinkandgo;

import java.util.ArrayList;

public class Cart 
{
	private final ArrayList<Drink> drinks;
	
	public Cart() 
	{	drinks = new ArrayList<Drink>();}
	
	public void addDrink(Drink drink)
	{	drinks.add(drink);}
	
	public void removeDrink(Drink drink)
	{	drinks.remove(drink);}

}
